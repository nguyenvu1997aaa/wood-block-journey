{
    "name": "game-core",
    "version": "1.0.0",
    "author": "Mr.Won",
    "license": "MIT",
    "sideEffects": false,
    "browserslist": "iOS >= 9, Android >= 5, Chrome >= 38 and not IE > 0 and not UCAndroid > 0 and not OperaMini > 0",
    "scripts": {
        "util:fix": "eslint -c configs/.eslintrc src/ --fix --ext .js,.ts --cache --cache-location configs/.eslintcache",
        "util:check": "eslint -c configs/.eslintrc src/ --ext .js,.ts",
        "util:unused": "ts-prune",
        "util:es5:bundle": "gulp validate-es5-build-files -in \"./build/game.*.js ./build/vendors.*.js ./build/engine.*.js\"",
        "util:es5:modules": "are-you-es5 check ./",
        "util:audio": "audiosprite --output \"assets/sounds/effects\" -f jukebox --export mp3 assets/original/sounds/*",
        "util:audio:optimal": "ffmpeg -y -i assets/sounds/effects.mp3 -codec:a libmp3lame -qscale:a 3 assets/sounds/effects-optimal.mp3",
        "util:music": "ffmpeg -y -i assets/original/music/bg_music.mp3 -codec:a libmp3lame -qscale:a 9 assets/music/bg_music.mp3",
        "util:compile:initFbInstant": "yarn babel --config-file ./configs/babel.config.json ./src/libs/initFbInstant.js --out-file ./src/libs/initFbInstant.es5.js",
        "util:language": "gulp language",
        "util:preBuild": "yarn util:compile:initFbInstant",
        "env:production": "cross-env-shell NODE_ENV=production",
        "env:development": "cross-env-shell NODE_ENV=development",
        "init-core": "gulp init-core",
        "format-code": "yarn prettier src/**/* --write && yarn util:fix",
        "generate-cert": "yarn devcert generate localhost",
        "dev": "yarn env:development snowpack --config snowpack.develop.mjs dev",
        "host": "yarn env:development snowpack --config snowpack.production.mjs dev",
        "embed": "yarn env:production webpack serve",
        "test:build": "yarn util:compile:initFbInstant && yarn env:production snowpack --config snowpack.production.mjs build",
        "build:prod": "yarn util:compile:initFbInstant && yarn env:production snowpack --config snowpack.production.mjs build",
        "build:phaser": "yarn env:production webpack --config ./phaser/webpack.phaser.js",
        "build:sentry": "yarn env:production SENTRY_DEPLOY=true webpack",
        "build:rollbar": "yarn env:production ROLLBAR_DEPLOY=true webpack",
        "build": "yarn util:compile:initFbInstant && yarn env:production CHECK_TS=true webpack && yarn util:es5:bundle",
        "deploy": "yarn util:compile:initFbInstant && yarn env:production CHECK_TS=true AUTO_VERSION=true webpack && yarn util:es5:bundle && gulp archive"
    },
    "alias": {
        "@/": "./src"
    },
    "eslintConfig": {
        "extends": [
            "./configs/.eslintrc"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 100,
        "trailingComma": "es5",
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf"
    },
    "dependencies": {
        "@0b5vr/tweakpane-plugin-profiler": "^0.3.0",
        "@babel/runtime": "^7.17.2",
        "abortcontroller-polyfill": "^1.7.3",
        "acorn": "^8.8.2",
        "bezier-easing": "^2.1.0",
        "glob": "^10.2.4",
        "immer": "^9.0.6",
        "merge-anything": "^5.0.2",
        "micro-memoize": "^4.0.9",
        "phaser": "3.55.2",
        "redux": "^4.1.2",
        "redux-thunk": "^2.4.1",
        "redux-watch": "^1.2.0",
        "reselect": "^4.1.5",
        "ts-debounce": "^4.0.0",
        "webfontloader": "^1.6.28",
        "whatwg-fetch": "^3.6.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.5",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-proposal-private-methods": "^7.16.11",
        "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@babel/runtime-corejs3": "^7.17.2",
        "@sentry/browser": "^6.17.9",
        "@sentry/types": "^6.17.9",
        "@sentry/webpack-plugin": "^1.18.7",
        "@snakemode/snowpack-html-loader-plugin": "^1.1.0",
        "@snowpack/plugin-dotenv": "^2.2.0",
        "@snowpack/plugin-optimize": "^0.2.13",
        "@snowpack/plugin-typescript": "^1.2.1",
        "@snowpack/plugin-webpack": "^3.0.0",
        "@tsconfig/recommended": "^1.0.1",
        "@tweakpane/core": "^1.0.8",
        "@types/node": "^17.0.19",
        "@types/redux-thunk": "^2.1.0",
        "@types/redux-watch": "^1.1.0",
        "@types/reselect": "^2.2.0",
        "@types/webfontloader": "^1.6.34",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "adm-zip": "^0.5.9",
        "are-you-es5": "^2.1.2",
        "audiosprite": "^0.7.2",
        "babel-loader": "^8.2.3",
        "bundle-stats-webpack-plugin": "^3.2.5",
        "clean-webpack-plugin": "^4.0.0",
        "client-zip": "^2.4.4",
        "copy-webpack-plugin": "^10.2.4",
        "cpy": "^8.1.2",
        "cross-env": "^7.0.3",
        "del": "^6.0.0",
        "devcert-cli": "^1.0.0",
        "dotenv-flow": "^3.2.0",
        "eruda": "^2.4.1",
        "es5-validator": "^1.3.1",
        "eslint": "^8.36.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "fork-ts-checker-webpack-plugin": "^7.2.1",
        "fs": "^0.0.1-security",
        "grunt-contrib-imagemin": "^4.0.0",
        "gulp": "^4.0.2",
        "gulp-zip": "^5.1.0",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "i18n-generator": "^0.5.1",
        "image-minimizer-webpack-plugin": "^3.2.3",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-mozjpeg": "^10.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-pngquant": "^9.0.2",
        "inline-source-webpack-plugin": "^2.0.1",
        "json-minimizer-webpack-plugin": "^3.3.0",
        "open": "^8.4.0",
        "prettier": "^2.5.1",
        "prettier-eslint": "^13.0.0",
        "request": "^2.88.2",
        "rollbar": "^2.25.0",
        "rollbar-sourcemap-webpack-plugin": "^3.3.0",
        "snowpack": "^3.8.8",
        "spectorjs": "^0.9.27",
        "terser-webpack-plugin": "^5.3.1",
        "ts-loader": "^9.2.6",
        "ts-prune": "^0.10.3",
        "tweakpane": "^3.0.7",
        "tweakpane-image-plugin": "^1.1.3",
        "typescript": "4.9.5",
        "uuid": "^8.3.2",
        "webpack": "^5.69.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    },
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    },
    "packageManager": "yarn@3.6.1"
}
